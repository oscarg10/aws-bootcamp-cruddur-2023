#! /usr/bin/bash


ABS_PATH=$(readlink -f "$0")
SERVERLESS_PATH=$(dirname $ABS_PATH)
DATA_FILE_PATH="$SERVERLESS_PATH/files/messi.jpeg"

aws s3 rm "$DATA_FILE_PATH" "s3://assets.$DOMAIN_NAME/avatars/original/messi.jpg"
aws s3 rm "$DATA_FILE_PATH" "s3://assets.$DOMAIN_NAME/avatars/processed/messi.png"